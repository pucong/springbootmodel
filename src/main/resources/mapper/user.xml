<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pc.dao.UserDao" >
    <select id="getUserById" resultType="java.util.HashMap">
        select * from SYS_USER  where id = #{id}
    </select>

    <select id="findByUserName" resultType="com.pc.model.SysUser" parameterType="java.lang.String">
        select * from SYS_USER  where username = #{name}
    </select>

</mapper>